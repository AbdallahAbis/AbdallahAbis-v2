{
	"presets": ["next/babel"],
	"plugins": [
		[
			"inline-react-svg",
			{
				"svgo": {
					"plugins": [
						{
							"removeAttrs": { "attrs": "(data-name)" }
						},
						{
							"cleanupIDs": false
						}
					]
				}
			}
		],
		[
			"styled-components",
			{
				"ssr": true,
				"displayName": true
			}
		]
	]
}
